import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc=new Scanner(System.in);
		int  n=sc.nextInt();
		int a[]=new int[n];
		int i,sum=0,o=0;
		for(i=0;i<n;i++)
		{
		    a[i]=sc.nextInt();
		}
		for(i=0;i<n;i++)
		{
		    if(a[i]%2==0)
		    {
		        sum=sum+a[i];
		    }
		    else
		    {
		        o=o+a[i];
		    }
		}
		System.out.print(o-sum);

	}
}
