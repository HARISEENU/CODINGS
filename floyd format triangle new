import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int i,j;
		int c=0,t=0;
		for(i=1;i<=n;i++)
		{
		    for(j=1;j<=i;j++)
		     {
		            t=c+j;
		            System.out.print(t+" ");    
		    }
		    c=t;
		    System.out.println();
		}

	}
}
