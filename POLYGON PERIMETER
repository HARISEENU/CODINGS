import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
Scanner sc=new Scanner(System.in);
int sum=0;
int i;
int n=sc.nextInt();
int a[]=new int[n];
for(i=0;i<n;i++)
{
    a[i]=sc.nextInt();
}
for(i=0;i<n;i++)
{
    sum=sum+a[i];
}
System.out.println(sum);
	}
}
OUTPUT
3
100
60
70
230
