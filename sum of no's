import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int N=sc.nextInt();
		int sum=0;
		int a[]=new int[N];
		for(int i=0;i<N;i++)
		{
		    a[i]=sc.nextInt();
		}
		for(int i=0;i<N;i++)
		{
		    sum=sum+a[i];
		}
		System.out.println(sum);

	}
}
OUTPUT
3
100
200
40
340
