import java.util.*;
public class Hello {

    public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
	int i,j;
  int c=1;
	int b=c;
  int t=n;
	for(i=n;i>=1;i--)
	{
	    for(j=1;j<=i;j++)
	    {
	        
	        System.out.print(b+" ");
	        b=b+t;
	        t--;
	    }
	    System.out.println();
	    t=n;
	    c++;
	    b=c;
	}

	}
}

INPUT
5

OUTPUT
1 6 10 13 15
2 7 11 14 
3 8 12 
4 9 
5

INPUT 
3

OUTPUT
1 4 6
2 5
3
